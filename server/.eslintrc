{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "no-param-reassign":"off",
      "no-console": "off",
      "func-names":"off",
      "max-len": "off",
      "camelcase":"off",
      "no-unused-vars":"off",
      "no-underscore-dangle": ["error", { "allow": ["_id", "_json"] }],
      "consistent-return":"off",
      "comma-dangle": [
        "error",
        {
          "functions": "ignore"
        }
      ]
    }
  }